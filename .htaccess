# Provide Specific Index Page
DirectoryIndex blog.php

<Files ~ "config\.ini$">
  Order allow,deny
  Deny from all
</Files>

RedirectMatch 404 /\.gitignore
RedirectMatch 404 /\.htaccess
RedirectMatch 404 /\README.md

RedirectMatch 404 /.git
RedirectMatch 404 /config
RedirectMatch 404 /controllers
RedirectMatch 404 /core
RedirectMatch 404 /database
RedirectMatch 404 /interfaces
RedirectMatch 404 /models
RedirectMatch 404 /templates
RedirectMatch 404 /utils
RedirectMatch 404 /views
